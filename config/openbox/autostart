# Automatyczne uruchamianie komponentów pulpitu

# Ustawienie rozdzielczości HD
xfsettingsd &

# Ustawienie tapety
nitrogen --restore &

# Panel XFCE z pluginami
xfce4-panel &

# Aplet sieci
nm-applet &

# Menedżer Bluetooth
blueman-applet &

# Kompozytor okien
compton &

# Menedżer zasilania z ikoną
xfce4-power-manager --no-daemon &

# Powiadomienia systemowe
xfce4-notifyd &


